angular.module('myApp.controllers', [])

.controller 'studentsCtrl', [
  '$scope'
  '$http'
  ($scope, $http) ->
    $http.get('/api/students').success (data, status, headers, config) ->
      console.log data
      console.log "!!!!!"
      $scope.students = _.chain(data).groupBy((element, index) ->
                        Math.floor index / 2
                      ).toArray().value()


]